python3 network.py train \
    --epochs 2 \
    --batch-size 3 \
    --iters-to-accumulate 4 \
    --learning-rate 0.001 \
    --max-disp 192 \
    --save ~/data/models/trash-1-2 \
    --load ~/data/models/sceneflow-1-1 \
    --dataset kittis \
    --root ~/data/datasets/ \
    --split 0.0 \
    --log-file ~/data/logs/trash-1-2.log \
    --iters-per-log 10